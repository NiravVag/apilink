@using DTO.Common;  
@{
    ViewData["Title"] = "Inspection Picking";
}

@model DTO.Kpi.ExportResult

<xls-sheet name="InspectionPicking">
    <table xls-begin="A:1" style="border-style:solid">
        <tr style="background-color:#f5f5f5;font-weight:bold;text-align:center">
            <th>Customer</th>
            <th>Supplier</th>
            <th>Factory</th>
            <th>Booking Number</th>
            <th>Service Date</th>
            <th>Po Number</th>
            <th>Product Id</th>
            <th>Product Category</th>
            <th>Product SubCategory</th>
            <th>Picking Qty</th>
            <th>Lab Name</th>
            <th>Tracking Number</th>
        </tr>
        @foreach (var item in Model.Data)
        {
        <tr>
            <td width="200px">@item.PickingCustomerName</td>
            <td width="200px">@item.PickingSupplierName</td>
            <td width="200px">@item.PickingFactoryName</td>
            <td width="150px" xls-type="number">@item.PickingInspectionId</td>
            <td width="150px" xls-type="date" xls-format="@ApiCommonData.StandardDateFormat">@item.PickingServiceDate</td>
            <td width="150px">@item.PickingPoNumber</td>
            <td width="150px">@item.PickingProductName</td>
            <td width="150px">@item.PickingProductCategory</td>
            <td width="150px">@item.PickingProductSubCategory</td>
            <td width="150px" xls-type="number">@item.PickingQuantity</td>
            <td width="150px">@item.PickingLabName</td>
            <td width="150px"></td>
        </tr>
        }
    </table>
</xls-sheet>


