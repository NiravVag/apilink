
@{
    ViewData["Title"] = "CombineOrders";
}


@model IEnumerable<DTO.CombineOrders.CombineOrders>

<xls-sheet name="Combine Orders Summary">
    <table xls-begin="A:1" style="border-style:solid">
        <tr style="background-color:#f5f5f5;font-weight:bold;text-align:center">
            <th>Product Id</th>
            <th>Po Name</th>
            <th>Product Description</th>
            <th>Factory Reference</th>
            <th>Combine Product Id</th>
            <th>Aql Level</th>
            <th>Aql Quantity</th>
            <th>Combined Aql Quantity</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr style="background-color:#f5f5f5;">
            <td height="200px" width="250px">@item.ProductName</td>
            <td height="200px" width="250px">@item.PoName</td>
            <td width="150px">@item.ProductDescription</td>
            <td width="250px">@item.FactoryReference</td>
            <td width="100px">@item.CombineProductName</td>
            <td width="150px">@item.AqlLevel</td>
            <td width="100px">@item.SamplingQuantity</td>
            <td width="150px">@item.CombinedAqlQuantity</td>

        </tr>
        }
    </table>
</xls-sheet>
